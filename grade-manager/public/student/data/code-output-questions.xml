<!-- Code Output Questions - For Loops -->

<question type="multichoice">
  <name><text>For Loop Output 1</text></name>
  <questiontext format="html">
    <text><![CDATA[Какво ще изведе този код?
<pre>
for (int i = 0; i < 3; i++)
{
    printf("%d ", i);
}
</pre>]]></text>
  </questiontext>
  <shuffleanswers>true</shuffleanswers>
  <answer fraction="100"><text>0 1 2</text></answer>
  <answer fraction="0"><text>0 1 2 3</text></answer>
  <answer fraction="0"><text>1 2 3</text></answer>
  <answer fraction="0"><text>0 2 4</text></answer>
  <tags><tag><text>loops</text></tag><tag><text>easy</text></tag></tags>
</question>

